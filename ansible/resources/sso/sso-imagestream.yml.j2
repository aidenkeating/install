#jinja2: lstrip_blocks: True
apiVersion: image.openshift.io/v1
kind: ImageStream
metadata:
  name: redhat-sso73-openshift
spec:
  lookupPolicy:
    local: false
  tags:
    {% for tag in sso_imagestream_tags %}
    - from:
        kind: DockerImage
        name: "{{ tag.image }}"
      name: "{{ tag.name }}"
      referencePolicy:
        type: Local
    {% endfor %}
